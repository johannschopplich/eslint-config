{
  "name": "@byjohann/eslint-config",
  "version": "0.2.0",
  "packageManager": "pnpm@8.7.5",
  "description": "ESLint config for my projects",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "repository": "johannschopplich/eslint-config",
  "license": "MIT",
  "keywords": [
    "eslint-config"
  ],
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint . --config=index.js",
    "format": "prettier \"**/*.{js,json,md,mjs,ts,yml}\" --write",
    "format:check": "prettier \"**/*.{js,json,md,mjs,ts,yml}\" --check",
    "release": "bumpp",
    "test": "pnpm run lint"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-vue": "^9.17.0"
  },
  "devDependencies": {
    "bumpp": "^9.2.0",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  }
}
